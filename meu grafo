list_graph=[]

class Graph():
    def __init__

class Node(Graph):
    def __init__(self, degree, neighbor):
        self.degree=degree
        self.neighbor=neighbor

    def get_degree(self):
        return self.degree

    def get_neighbor(self):
        return self.neighbor

for Graph in list_graph:
    if Graph != []:
        pass
    else: 
        print("error")


class Nodesub(Node):
    def __init__(self, degree, name):
        super().__init__(degree)
        self.name=name 

class Edge():
    def __init__(self, node1, node2):
        self.node1=node1
        self.node2=node2
