class Universidade():
    def __init__(self, univname, cidade, degree):
        self.univname=univname
        self.cidade=cidade
        self.degree=degree
    def get_univname(self):
        return self.univname
    def get_cidade(self):
        return self.cidade
    def get_degree(self):
        return self.degree
    
